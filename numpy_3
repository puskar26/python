# Mathematics

import numpy as np

a=np.array([1,2,3])
b=np.array([4,5,6])

#DMAS an array by any number
print(a + 2)
print(a - 2)
print(a * 2)
print(a / 2)

#DMAS two arrays
print(a + b)
print(a - b)
print(a * b)
print(b / a)

#Powers
print(a ** 2)
print(a ** b)

#Functions
print(np.cos(a))
print(np.sin(a))
print(np.log(a))
